<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asamles.app" >
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.SEND_SMS" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.asamles.app.MainActivity"
            android:label="@string/app_name"
            android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity 
			android:name="com.example.asamles.app.upNavigation.UpMain" android:theme="@style/noAnimTheme">
		</activity>
		<activity 
			android:name="com.example.asamles.app.upNavigation.UpFirst"
			android:parentActivityName="com.example.asamles.app.upNavigation.UpMain" android:theme="@style/noAnimTheme">
		</activity>
		<activity 
			android:name="com.example.asamles.app.upNavigation.UpSecond"
			android:parentActivityName="com.example.asamles.app.upNavigation.UpMain">
		</activity>
		
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
